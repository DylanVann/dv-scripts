{
  "name": "dv-scripts",
  "version": "1.0.8",
  "description": "Scripts for Node/TypeScript development.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanvann/dv-scripts.git",
    "directory": "packages/dv-scripts"
  },
  "license": "MIT",
  "bin": {
    "dv-scripts": "./bin.js"
  },
  "files": [
    "bin.js",
    "dist",
    "ignore",
    "!dist/**/*.d.ts.map"
  ],
  "scripts": {
    "build": "yarn run build:typescript && yarn run build:copy-readme",
    "build:copy-readme": "cp ../../README.md ./",
    "build:typescript": "tsc --build",
    "lint": "node bin.js lint",
    "start": "tsc --build --watch",
    "test": "node bin.js test"
  },
  "eslintConfig": {
    "extends": "dv-scripts"
  },
  "dependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@qiwi/multi-semantic-release": "^3.0.2",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/npm": "^7.0.5",
    "@types/prettier": "^2.0.1",
    "babel-jest": "24.9.0",
    "babel-plugin-emotion": "^10.0.33",
    "conventional-changelog-cli": "^2.0.34",
    "eslint": "^6.8.0",
    "eslint-config-dv-scripts": "^1.0.0",
    "execa": "^4.0.1",
    "find-yarn-workspace-root": "^2.0.0",
    "is-ci": "^2.0.0",
    "jest": "24.9.0",
    "pkg-dir": "^4.1.0",
    "prettier": "^2.0.5",
    "rollup": "^2.18.2",
    "semantic-release": "^17.1.1",
    "sort-package-json": "^1.44.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/execa": "^2.0.0",
    "@types/is-ci": "^2.0.0",
    "@types/node": "^14.0.14",
    "typescript": "^3.9.6"
  }
}
